# 启用 DEBUG_FS
CONFIG_CMA_DEBUGFS=y
CONFIG_REGMAP_ALLOW_WRITE_DEBUGFS=y
CONFIG_QMP_DEBUGFS_CLIENT=y
CONFIG_MSM_TZ_LOG=y
CONFIG_DEBUG_FS=y

# 启用 CFQ_SSD_OPT 优化
# ADD: 废弃方案 在提交 b79f123e5b58 还原
# CONFIG_CFQ_SSD_OPT is not set
# END: 废弃方案 在提交 b79f123e5b58 还原
CONFIG_SSD_OPT=y

# 启用 ZRAM_WRITEBACK 回写特性
CONFIG_ZRAM_WRITEBACK=y
CONFIG_ZRAM_MEMORY_TRACKING=y
# ADD: 禁用队列深度 MQ调度器
# CONFIG_MQ_IOSCHED_KYBER is not set
# END: 禁用队列深度 MQ调度器

# ADD: MIUI vendor 模式的WIFI驱动模式
CONFIG_MACH_XIAOMI_PSYCHE_MIUI_VENDOR=y
# END: MIUI vendor 模式的WIFI驱动模式
